{"id":"proj_1768053741319","name":"Player topdown com sprites","mode":"2d","lastModified":1768056436098,"thumbnail":"linear-gradient(135deg, hsl(191, 11%, 15%), hsl(211, 1%, 25%))","scenes":{"scene_initial":{"id":"scene_initial","name":"inicial.clr","objects":[{"id":"obj_1768053747338_fsde9","name":"Joystick1","uniqueTag":"#D8zKJ","type":"joystick","position":{"x":150,"y":600},"rotation":0,"color":"#ffffff","opacity":1,"visible":true,"locked":false,"scripts":[],"variables":{},"textureTiling":false,"tag":"joystick","scale":{"x":2.5,"y":2.5},"zIndex":1,"joystick":{"x":0,"y":0,"isTouching":false},"physicsEnabled":false,"collisionEnabled":false,"initialState":{"uniqueTag":"#D8zKJ","type":"joystick","position":{"x":858.0315742904017,"y":335.28638088451993},"rotation":0,"color":"#ffffff","opacity":1,"visible":true,"locked":false,"scripts":[],"variables":{},"textureTiling":false,"tag":"joystick","scale":{"x":2.5,"y":2.5},"zIndex":1,"joystick":{"x":0,"y":0,"isTouching":false},"physicsEnabled":false,"collisionEnabled":false}},{"id":"obj_1768053770049_u2v3l","name":"Sprite1","uniqueTag":"#eQjKp","type":"sprite","position":{"x":858.0315742904017,"y":335.28638088451993},"rotation":0,"color":"#ffffff","opacity":1,"visible":true,"locked":false,"scripts":[{"id":"script_1768053797120","name":"8_way_movement.js","type":"javascript","content":"// --- MOVIMENTAÇÃO EM 8 DIREÇÕES (TOP-DOWN) ---\n// Ideal para jogos com visão de cima ou com gravidade zero.\n\nlet joystick;\nconst WALK_ANIM = \"andando\";\nconst IDLE_ANIM = \"parado\";\nconst MOVE_SPEED = 300;\n\nlet originalScaleX;\n\nfunction start() {\n    joystick = find(\"joystick\");\n    originalScaleX = this.scale.x;\n    this.animation.play(IDLE_ANIM);\n}\n\nfunction update(deltaTime) {\n    if (!this.physics || !joystick) return;\n\n    let isMoving = false;\n\n    if (joystick.joystick.isTouching) {\n        const inputX = joystick.joystick.x;\n        const inputY = joystick.joystick.y;\n        const magnitude = Math.sqrt(inputX * inputX + inputY * inputY);\n        \n        if (magnitude > 0.1) {\n            isMoving = true;\n            const dirX = inputX / magnitude;\n            const dirY = inputY / magnitude;\n            this.physics.setVelocity(dirX * MOVE_SPEED, dirY * MOVE_SPEED);\n\n            if (inputX > 0.1) this.scale = { x: Math.abs(originalScaleX), y: this.scale.y };\n            else if (inputX < -0.1) this.scale = { x: -Math.abs(originalScaleX), y: this.scale.y };\n        } else {\n             this.physics.setVelocity(0, 0);\n        }\n    } else {\n        this.physics.setVelocity(0, 0);\n    }\n\n    if (isMoving) this.animation.play(WALK_ANIM);\n    else this.animation.play(IDLE_ANIM);\n}"}],"variables":{},"textureTiling":false,"tag":"sprite","scale":{"x":1.5,"y":1.5},"zIndex":0,"physicsEnabled":true,"collisionEnabled":true,"isStatic":false,"isKinematic":false,"lockRotation":true,"mass":1,"friction":0.7,"collisionShape":"circle","useCustomCollision":true,"collisionBoxScale":{"x":1.0571432495117188,"y":1.0571432495117188},"collisionBoxOffset":{"x":0,"y":0},"initialState":{"uniqueTag":"#eQjKp","type":"sprite","position":{"x":858.0315742904017,"y":335.28638088451993},"rotation":0,"color":"#ffffff","opacity":1,"visible":true,"locked":false,"scripts":[],"variables":{},"textureTiling":false,"tag":"sprite","scale":{"x":1.5,"y":1.5},"zIndex":0,"physicsEnabled":true,"collisionEnabled":true,"isStatic":false,"isKinematic":false,"lockRotation":false,"mass":1,"friction":0.7,"collisionShape":"box","useCustomCollision":false,"collisionBoxScale":{"x":1,"y":1},"collisionBoxOffset":{"x":0,"y":0}},"texture":"data:image/webp;base64,","animations":[{"id":"anim_1768055660930","name":"parado","frames":["data:image/webp;base64,","data:image/webp;base64,","data:image/webp;base64,","data:image/webp;base64,","data:image/webp;base64,","data:image/webp;base64,","data:image/webp;base64,","data:image/webp;base64,"],"speed":10,"loop":true},{"id":"anim_1768055706407","name":"andando","frame